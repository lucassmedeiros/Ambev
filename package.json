{
  "name": "addons-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode standalone",
    "dev:db": "json-server --watch db.json --port 3000",
    "build": "vite build",
    "build-sa": "vite build --mode standalone",
    "preview": "concurrently \"vite preview --host 0.0.0.0\" \"vite build --watch\"",
    "preview-ssl": "concurrently \"vite preview --mode ssl -l silent\" \"vite build --watch\"",
    "test": "vitest run --coverage --passWithNoTests",
    "test:local": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^4.1.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.9.0",
    "@mui/styles": "^5.9.2",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "single-spa": "^6.0.1",
    "single-spa-react": "^6.0.1",
    "styled-components": "^6.1.15",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "i18next": "^23.11.4",
    "jest": "^29.7.0",
    "jsdom": "^24.1.1",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "react-i18next": "^15.0.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  }
